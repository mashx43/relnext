{
	"name": "@mash43/relnext",
	"version": "0.1.0",
	"description": "A TypeScript library designed to detect pagination links such as \"next\" and \"previous\" from web page HTML content or URLs.",
	"author": "mash43",
	"license": "MIT",
	"homepage": "https://github.com/mashx43/relnext#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mashx43/relnext.git"
	},
	"bugs": {
		"url": "https://github.com/mashx43/relnext/issues"
	},
	"keywords": [
		"pagination",
		"next",
		"prev",
		"rel-next"
	],
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"test": "bun test",
		"typecheck": "tsc --noEmit --skipLibCheck"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "^1.3.6",
		"tsdown": "^0.20.1",
		"typescript": "^5"
	}
}
